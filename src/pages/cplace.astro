---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
const { title, img, desc, url, badge, tags, target = "_blank" } = Astro.props;
import { Image } from "astro:assets";
import CPlaceCover from "../../public/CPlaceCover.png";
---

<BaseLayout title = "C/Place" sideBarActiveItemID="projects">
    <div>
        <div class="text-3xl w-full font-bold mb-5">C/Place: Collaborative Pixel-Art Board</div>
    </div>

    <div
    class="rounded-lg bg-base-500"
    >

    <img
        src={CPlaceCover.src}
        width="1500px"
        height="1000px"
        format="webp"
        alt={"CPlaceCover"}
        class="max-w-full md:max-w-[13rem] rounded-lg"
    />
   
    </div>

</BaseLayout>
