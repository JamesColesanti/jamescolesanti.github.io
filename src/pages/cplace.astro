---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
const { title, img, desc, url, badge, tags, target = "_blank" } = Astro.props;
import { Image } from "astro:assets";
---

<BaseLayout title = "C/Place" sideBarActiveItemID="projects">
    <div>
        <div class="text-3xl w-full font-bold mb-5">C/Place: Collaborative Pixel-Art Board</div>
    </div>

    <div
    class="rounded-lg bg-base-100 hover:shadow-xl transition ease-in-out hover:scale-[102%]"
    >

    <Image
        src={"../../CPlaceCover.png"}
        width={750}
        height={422}
        format="webp"
        alt={title}
        class="max-w-full md:max-w-[13rem] rounded-lg"
    />
   
    </div>

</BaseLayout>
